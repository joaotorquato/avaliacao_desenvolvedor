<section class="list-list">
  <div class="row">
    <div class="col-md-offset-3 col-md-6 col-sm-12 text-center">
      <div class="page-header">
        <h1>
          Registers History
        </h1>
      </div>
    </div>
  </div>
  <div class="row">
    <div class="col-md-offset-3 col-md-6 col-sm-12">
      <div class="panel panel-default">
        <div class="panel-heading">
          Registers
        </div>
        <div class="panel-body">
          <table class="table table-striped task-table">
            <thead>
              <tr>
                <td>Comprador</td>
                <td>Descrição</td>
                <td>Preço Uniário</td>
                <td>Quantidade</td>
                <td>Endereço</td>
                <td>Fornecedor</td>
              </tr>
            </thead>
            <tbody>
              <%= render @registers %>
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
</section>
