<div class="row">
  <div class="col-md-offset-3 col-md-6 col-sm-12">
    <div class="page-header text-center">
      <h1>Upload a new register file</h1>
    </div>
    <div class="panel panel-default">
      <div class="panel-heading">
        Upload
      </div>
      <div class="panel-body">
        <%= simple_form_for :register, { url: registers_path, method: :post } do |f| %>
          <%= f.input :file, label: 'Upload a register file', wrapper_html: { class: 'form-group' }, as: :file %>
          <%= button_tag 'Save', type: :submit, class: 'btn btn-primary' %>
        <% end %>
      </div>
    </div>
  </div>
</div>
